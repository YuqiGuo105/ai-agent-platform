# Database Configuration
DB_URL=jdbc:postgresql://your-host:port/database
DB_USERNAME=postgres
DB_PASSWORD=your_database_password

# RabbitMQ Configuration
RABBITMQ_URL=amqp://username:password@your-host:port
RABBITMQ_USER=your_rabbitmq_username
RABBITMQ_PASS=your_rabbitmq_password

# Redis Configuration
REDIS_URL=redis://default:your_redis_password@your-host:port

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_CHAT_MODEL=gpt-4.1-mini
OPENAI_TEMPERATURE=0.3
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIMENSIONS=1536

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_CHAT_MODEL=deepseek-chat
DEEPSEEK_TEMPERATURE=0.2

# Google Gemini Configuration (optional)
GEMINI_API_KEY=your_gemini_api_key
GEMINI_CHAT_MODEL=gemini-2.0-flash
GEMINI_TEMPERATURE=0.3

# DashScope (Qwen) Configuration
DASHSCOPE_API_KEY=your_dashscope_api_key
DASHSCOPE_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1
DASHSCOPE_CHAT_MODEL=qwen-vl-plus
QWEN_TEMPERATURE=0.1
QWEN_MAX_TOKENS=2048
QWEN_ENABLE_THINKING=false

# Vision Model Configuration for File Content Extraction
# Options: "qwen-vl" (default) or "openai"
# "qwen-vl" uses Alibaba DashScope Qwen VL model
# "openai" uses OpenAI Vision API (gpt-4o-mini)
VISION_PROVIDER=qwen-vl

# PGVector Configuration
PGVECTOR_DIMENSIONS=1536

# S3/Tigris Storage Configuration
S3_ENDPOINT=https://t3.storageapi.dev
S3_REGION=auto
S3_BUCKET=your_bucket_name
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
S3_PRESIGN_TTL_SECONDS=900

# Server Configuration
PORT=8080

# CORS Configuration
CORS_ALLOWED_ORIGINS=*

# Elasticsearch Configuration
ELASTIC_BASE_URL=your_elasticsearch_url
ELASTIC_USERNAME=your_elastic_username
ELASTIC_PASSWORD=your_elastic_password
ELASTIC_STORE_TO_ES=true
ELASTIC_ES_SYNC=true
ELASTIC_ES_INDEX=mrpot_candidates

# =====================================================
# Docker Configuration (for docker-compose)
# =====================================================
# When using docker-compose, these values are automatically set
# in the compose file. Only API keys need to be provided here.
#
# Local Docker (docker-compose up):
#   DB_URL=jdbc:postgresql://postgres:5432/aiagent
#   REDIS_URL=redis://redis:6379
#   RABBITMQ_URL=amqp://aiagent:aiagent_pwd@rabbitmq:5672
#   ELASTIC_BASE_URL=http://elasticsearch:9200
#   TOOLS_SERVICE_URL=http://agent-tools-service:8081
#
# Required API Keys for Docker:
#   DEEPSEEK_API_KEY=your_deepseek_api_key
#   OPENAI_API_KEY=your_openai_api_key (optional)
#   GEMINI_API_KEY=your_gemini_api_key (optional)
#   DASHSCOPE_API_KEY=your_dashscope_api_key (for Qwen VL)
